			</nav>



		{{#each stories}}
			<div class="story" id="sample">
				<a href="/story/{{title}}"><h><img class="thumb img-rounded" src="images/W.jpg">
				<div class="titleprog">
					<span style="overflow:hidden">{{title}}</span></h></a>
				</div>
			</div>
		{{/each}}
	
		</div>
		<script src="js/jquery-1.11.0.js"></script>
		<script src="js/bootstrap.js"></script>
		<script src="js/storyshare.js"></script>
